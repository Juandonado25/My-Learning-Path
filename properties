using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace propertiesExercises
{
    class Box
    {
        public int surface;
        public int Length { get; set; }
        public int Height { get; set; }

        public Box(int length, int height)
        {
            Length = length;
            Height = height;
        }

        public int Surface
        {
            get { return Height * Length; ; }
            set { this.Surface = value; }
        }
    }
}
